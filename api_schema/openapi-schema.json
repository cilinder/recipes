            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "maxLength": 100
                },
                "quantity": {
                  "type": "number"
                },
                "unit": {
                  "type": "string",
                  "maxLength": 5
                }
              },
              "required": [
                "name",
                "quantity",
                "unit"
              ]
            },
            "readOnly": true
          }
        },
        "required": [
